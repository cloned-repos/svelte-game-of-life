{
	"name": "svelte-game-of-life",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"test:unit:coverage": "vitest --coverage"
	},
	"dependencies": {
		"@easyfonts/font-awesome-v6": "6.0.6",
		"@easyfonts/league-junction-typeface": "1.0.3",
		"@fontsource/fira-mono": "5.0.2",
		"@fontsource/lato": "5.0.3",
		"@fontsource/open-sans": "5.0.3",
		"@mangos/debug-frontend": "0.0.4",
		"@reduxjs/toolkit": "2.2.5"
	},
	"devDependencies": {
		"@playwright/test": "1.34.3",
		"@sveltejs/adapter-auto": "3.0.0",
		"@sveltejs/adapter-node": "2.0.0",
		"@sveltejs/adapter-static": "3.0.0",
		"@sveltejs/kit": "2.0.0",
		"@sveltejs/vite-plugin-svelte": "3.0.0",
		"@types/cookie": "0.5.1",
		"@types/opentype.js": "1.3.5",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"@vitest/coverage-v8": "1.6.0",
		"autoprefixer": "10.4.14",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"install": "0.13.0",
		"jsdom": "22.1.0",
		"npm": "9.7.1",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"serve": "14.2.0",
		"svelte": "4.0.0",
		"svelte-check": "3.4.3",
		"svelte-remove-attributes": "0.0.3",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.2",
		"typescript": "5.1.3",
		"vite": "^5.0.0",
		"vitest": "1.6.0"
	}
}
